{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:graphql": "yarn build:graphql --watch",
    "dev:path": "pathpida --ignorePath .gitignore --output src/lib/pathpida --watch",
    "build": "run-s build:path build:graphql build:next",
    "build:next": "next build",
    "build:path": "pathpida --ignorePath .gitignore --output src/lib/pathpida",
    "build:graphql": "graphql-codegen --require dotenv/config --config ./codegen.ts",
    "start": "next start",
    "lint": "run-p lint:*",
    "lint:next": "next lint --dir src --ignore-path .prettierignore --max-warnings 0",
    "lint:prettier": "prettier --check .",
    "lint:typecheck": "tsc --pretty --noEmit",
    "format": "run-s format:eslint format:prettier",
    "format:eslint": "yarn lint:next --fix",
    "format:prettier": "yarn lint:prettier --write",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.10",
    "date-fns": "^2.29.3",
    "graphql": "^16.6.0",
    "graphql-request": "^5.1.0",
    "next": "13.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.41.0",
    "typesafe-utils": "^1.16.2",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.1",
    "@graphql-codegen/near-operation-file-preset": "^2.4.4",
    "@graphql-codegen/typed-document-node": "^2.3.10",
    "@graphql-codegen/typescript": "^2.8.5",
    "@graphql-codegen/typescript-operations": "^2.5.10",
    "@tailwindcss/typography": "^0.5.8",
    "@tsconfig/strictest": "^1.0.2",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "autoprefixer": "^10.4.13",
    "daisyui": "^2.46.0",
    "eslint": "8.30.0",
    "eslint-config-next": "13.1.0",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.20.1",
    "postcss": "^8.4.20",
    "postcss-custom-media": "^9.0.1",
    "prettier": "^2.8.1",
    "tailwindcss": "^3.2.4",
    "typescript": "4.9.4"
  },
  "volta": {
    "node": "18.12.1",
    "yarn": "1.22.19"
  }
}
